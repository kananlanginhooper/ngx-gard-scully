{
  "name": "ngx-gard-skully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "ng build --prod",
    "scully": "scully",
    "scully:serve": "scully serve",
    "fetch:fromAPI": "cd GARD_Node_SFDownload && npm i && node app.js && cd ..",
    "fetch:copy": "cd GARD_Node_SFDownload && cp diseases*json ../src/assets && cp -r singles ../src/assets && cd ..",
    "fetch": "npm run fetch:fromAPI && npm run fetch:copy",
    "renderRoutes": "npm run scully --scanRoutes",
    "render:WithFetch": "npm run fetch && npm run prepare && npm run renderRoutes",
    "render:NoFetch": "npm run prepare && npm run renderRoutes",
    "serve": "scully serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
